{
  "name": "synapse-copycat",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/synapse-copycat/src",
  "prefix": "synapse-copycat",
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/apps/synapse-copycat",
        "index": "apps/synapse-copycat/src/index.html",
        "main": "apps/synapse-copycat/src/main.ts",
        "polyfills": [
          "zone.js"
        ],
        "tsConfig": "apps/synapse-copycat/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          "apps/synapse-copycat/src/favicon.ico",
          "apps/synapse-copycat/src/assets",
          "apps/synapse-copycat/src/assets/images"
        ],
        "styles": [
          "apps/synapse-copycat/src/styles.scss"
        ],
        "scripts": []
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "buildTarget": "synapse-copycat:build:production"
        },
        "development": {
          "buildTarget": "synapse-copycat:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "buildTarget": "synapse-copycat:build"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": [
        "{options.outputFile}"
      ]
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "apps/synapse-copycat/jest.config.ts"
      }
    },
    "storybook": {
      "executor": "@storybook/angular:start-storybook",
      "options": {
        "port": 4400,
        "configDir": "apps/synapse-copycat/.storybook",
        "browserTarget": "synapse-copycat:build",
        "compodoc": true,
        "compodocArgs": [
          "-e",
          "json",
          "-d",
          "apps/synapse-copycat"
        ]
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    },
    "build-storybook": {
      "executor": "@storybook/angular:build-storybook",
      "outputs": [
        "{options.outputDir}"
      ],
      "options": {
        "outputDir": "dist/storybook/synapse-copycat",
        "configDir": "apps/synapse-copycat/.storybook",
        "browserTarget": "synapse-copycat:build",
        "compodoc": true,
        "compodocArgs": [
          "-e",
          "json",
          "-d",
          "apps/synapse-copycat"
        ]
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    }
  },
  "tags": []
}
